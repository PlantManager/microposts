<h1>プロフィール編集画面</h1>

<div>
<%= form_for(@user) do |f| %>
    
    名前:
    <%= f.text_field :name %>
     
    都道府県:
    <%= f.text_field :area %>
    
    プロフィール:
    <%= f.text_area  :profile , :cols => 40, :rows => 5 %>
    
    メール:
    <%= f.text_field :email %>
  
    <%= f.submit  %>

  <% end %>
<hr>
<p>views/users/edit.html.erb</p>
</div>
